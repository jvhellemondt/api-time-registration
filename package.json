{
  "name": "time-registration",
  "module": "src/timeRegistration.module.ts",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --hot --watch src/main.ts",
    "build": "bun build ./src/main.ts --splitting --outdir=./dist --target=bun --tsconfig=tsconfig.build.json",
    "start": "NODE_ENV=production bun dist/main.js",
    "prepare": "husky",
    "commit": "cz",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --coverage --watch",
    "test:ui": "NODE_ENV=test vitest --watch --coverage --ui",
    "coverage": "NODE_ENV=test vitest --coverage",
    "typecheck": "tsc --noEmit",
    "release": "release-it"
  },
  "dependencies": {
    "@jvhellemondt/arts-and-crafts.ts": "link:@jvhellemondt/arts-and-crafts.ts",
    "date-fns": "^4.1.0",
    "hono": "^4.8.5",
    "mongodb": "^6.17.0",
    "ts-case-convert": "^2.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@release-it/bumper": "^7.0.5",
    "@stylistic/eslint-plugin": "^5.2.0",
    "@types/bun": "^1.2.18",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "changelogen": "^0.6.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "release-it": "^19.0.4",
    "release-it-changelogen": "^0.1.0",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "bun": {
    "target": "esnext"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
