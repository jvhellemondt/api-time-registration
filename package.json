{
  "name": "time-registration",
  "module": "src/timeRegistration.module.ts",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --hot --watch src/main.ts",
    "build": "bun build ./src/main.ts --splitting --outdir=./dist --target=bun --tsconfig=tsconfig.build.json",
    "start": "NODE_ENV=production bun dist/main.js",
    "prepare": "husky",
    "commit": "cz",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --coverage --watch",
    "test:ui": "NODE_ENV=test vitest --watch --coverage --ui",
    "coverage": "NODE_ENV=test vitest --coverage",
    "typecheck": "tsc --noEmit",
    "release": "release-it"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "date-fns": "^4.1.0",
    "effect": "^3.16.5",
    "hono": "^4.7.10",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@release-it/bumper": "^7.0.5",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/bun": "^1.2.13",
    "@vitest/coverage-istanbul": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "changelogen": "^0.6.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "release-it": "^19.0.2",
    "release-it-changelogen": "^0.1.0",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "bun": {
    "target": "esnext"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
